;&nbsp;set master_hosp;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;src='hp';</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;array dxs $ dx_code_1 - dx_code_25;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;array cdxs $ conv_dx_code_1 - conv_dx_code_25;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;format type $5.;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;format code $10.;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;do i = 1 to 25;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;if dxs(i) in: (&amp;pregCodes) then do;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;output;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;leave;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;end;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;if cdxs(i) in: (&amp;pregCodes) then do;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;output;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;leave;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;end;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;end;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;keep key_hsn adm_date dschg_date src;</div><div class=\"ql-code-block\">run;</div><div class=\"ql-code-block\">data VXI26b;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;set master_msb;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;src='mb';</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;if diag in:(&amp;pregCodes) then output;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;adm_date=dos;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;dschg_date=dos;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;keep key_hsn adm_date dschg_date src;</div><div class=\"ql-code-block\">run;</div><div class=\"ql-code-block\">data VXI26c;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;set VXI26a VXI26b;</div><div class=\"ql-code-block\">run;</div><div class=\"ql-code-block\">/*link to VXI25, indicate those having preg during pre365*/</div><div class=\"ql-code-block\">%sqljoinv2(</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;t1=VXI26c,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;t2=VXI25,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;target=VXI26d,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;distinct=1,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;vars=%str(t1.key_hsn, 1 as preg_pre365),</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;jtype=inner,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;jvars=key_hsn,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;filters=%str(&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;t1.adm_date between t2.entryDate -1 and t2.entryDate - &amp;runindays</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;or</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;t1.dschg_date between t2.entryDate -1 and t2.entryDate - &amp;runindays</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;or </div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;(t1.adm_date &lt; t2.entryDate-&amp;runindays and t1.dschg_date &gt; t2.entryDate -1)</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;)</div><div class=\"ql-code-block\">);</div><div class=\"ql-code-block\">/*link to VXI25, indicate those having preg during post365*/</div><div class=\"ql-code-block\">%sqljoinv2(</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;t1=VXI26c,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;t2=VXI25,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;target=VXI26e,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;distinct=1,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;vars=%str(t1.key_hsn, 1 as preg_post365),</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;jtype=inner,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;jvars=key_hsn,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;filters=%str(&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;t1.adm_date between t2.entryDate and t2.entryDate + &amp;followdays -1</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&