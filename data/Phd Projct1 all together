rs=%str(t1.key_hsn, t1.Rx_ddddd, t1.key_prscr_num),</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;jtype=inner,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;jvars=din,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;filters=%str(docslushcode='N')</div><div class=\"ql-code-block\">);</div><div class=\"ql-code-block\"><br></div><div class=\"ql-code-block\">/*claims of antiHTN*/</div><div class=\"ql-code-block\">%sqljoinv2(</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;t1=master_dp_benefit,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;t2=locald.ahtndins,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;distinct=1,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;target=VXI27a3,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;vars=%str(t1.key_hsn, t1.Rx_ddddd, t1.key_prscr_num),</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;jtype=inner,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;jvars=din,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;filters=%str(docslushcode='N')</div><div class=\"ql-code-block\">);</div><div class=\"ql-code-block\">%sqljoinv2(</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;t1=master_dp_nonbenefit,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;t2=locald.ahtndins,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;distinct=1,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;target=VXI27a4,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;vars=%str(t1.key_hsn, t1.Rx_ddddd, t1.key_prscr_num),</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;jtype=inner,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;jvars=din,</div><div class=\"ql-code-block\">&nbsp;&nbsp;&nbsp;&nbsp;filters=%str(docslushcode='N')</div><div class=\"ql-code-block\">);</div><div class=\"ql-code-block\"><br></div><div class=\"ql-code-block\">/*claims of oh*/</div><div class=\"ql-code-block\">%sqljoin