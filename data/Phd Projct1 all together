S = scan(UCPF1, 4, \":\");</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;if docSex_FS='M' then docSexN_FS=0;</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;else docSexN_FS=1;</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;docBirthYr_FS= scan(UCPF1, 5, \":\");</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;docAge_FS=int(yrdif(mdy(6,30,docbirthyr_FS), entryDate, \"age\"));</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;docGradYr_FS= input(scan(UCPF1, 6, \":\"), 8.);</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;docServiceYr_FS= year(entryDate) - docGradYr_FS;</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;docEdCountry_FS = input(scan(UCPF1, 8, \":\"), 8.); /*0 for foreign*/</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;docPayType_FS = scan(UCPF1, 9, \":\");</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;if strip(docPayType_FS )='NFFS' then docPayTypeN_FS =1;</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;else do;</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;if strip(docPayType_FS )='FFS' then docPayTypeN_FS =0;</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;else docPayTypeN_FS =2;/*unknown*/</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;end;</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;docN_Visits_FS = input(scan(UCPF1, 10, \":\"), 8.); /*N_Visits*/</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;docNPatients_FS = input(scan(UCPF1, 11, \":\"), 8.);</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;docNRx_FS= input(scan(UCPF1, 12, \":\"), 8.);</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;docNStatinRx_FS= input(scan(UCPF1, 13, \":\"), 8.);</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;if docNStatinRx_FS=. then docNStatinRx_FS=0;</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;if docNRx_FS=. or docNRx_FS=0 or docNStatinRx_FS=0 then docStatinRxPCT_FS=0;</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;else docStatinRxPCT_FS = round(docNStatinRx_FS/docNRx_FS*100, '0.01');</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;docNPatients_FS100=docNPatients_FS/100;</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;docN_Visits_FS100 =docN_Visits_FS/100;</div><div class=\"ql-code-block\" data-language=\"plain\">run;</div><div class=\"ql-code-block\" data-language=\"plain\">/*Define psVars*/</div><div class=\"ql-code-block\" data-language=\"plain\">%let psVars =&nbsp;&nbsp;&nbsp;&nbsp;</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;QAIPPEN</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;RESITYPEN</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;NDINS_PRE365</div><div class=\"ql-code-block\" data-language=\"plain\">/*&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;NDINS_POST365*/</div><div class=\"ql-code-block\" data-language=\"plain\">/*&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;NHOSP_PRE365*/</div><div class=\"ql-code-block\" data-language=\"plain\">/*&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;NHOSP_POST365*/</div><div class=\"ql-code-block\" data-language=\"plain\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;HNIGHTS_PRE365</div><div class=\"ql-code-block